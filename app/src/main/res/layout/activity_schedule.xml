<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.simonungar.overlap320.Schedule">

    <EditText
        android:id="@+id/editText15"
        android:layout_width="136dp"
        android:layout_height="40dp"
        android:layout_alignBaseline="@+id/editText16"
        android:layout_alignBottom="@+id/editText16"
        android:layout_alignStart="@+id/editText4"
        android:ems="10"
        android:hint="time"
        android:inputType="time"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="84dp" />

    <EditText
        android:id="@+id/editText16"
        android:layout_width="136dp"
        android:layout_height="40dp"
        android:layout_above="@+id/editText18"
        android:layout_alignEnd="@+id/editText7"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:hint="date"
        android:inputType="date"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="84dp" />

    <EditText
        android:id="@+id/editText17"
        android:layout_width="136dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/button3"
        android:layout_below="@+id/editText18"
        android:layout_marginTop="7dp"
        android:ems="10"
        android:hint="time"
        android:inputType="time"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="132dp" />

    <EditText
        android:id="@+id/editText18"
        android:layout_width="136dp"
        android:layout_height="40dp"
        android:layout_above="@+id/button3"
        android:layout_alignStart="@+id/editText16"
        android:layout_marginBottom="9dp"
        android:ems="10"
        android:hint="date"
        android:inputType="date"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="132dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="145dp"
        android:layout_height="49dp"
        android:layout_above="@+id/editText19"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="22dp"
        android:text="Add Event"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="194dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="145dp"
        android:layout_height="49dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/editText17"
        android:layout_marginBottom="87dp"
        android:text="Delete Event"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="412dp" />

    <EditText
        android:id="@+id/editText19"
        android:layout_width="288dp"
        android:layout_height="48dp"
        android:layout_above="@+id/editText20"
        android:layout_alignStart="@+id/editText4"
        android:layout_marginBottom="33dp"
        android:ems="10"
        android:inputType="textMultiLine|textShortMessage"
        android:text="7:00 am - 9:00 am "
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="276dp" />

    <EditText
        android:id="@+id/editText20"
        android:layout_width="288dp"
        android:layout_height="48dp"
        android:layout_above="@+id/button4"
        android:layout_alignStart="@+id/editText19"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:inputType="textNoSuggestions|textShortMessage"
        android:text="5:30 pm - 6:30 pm "
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="348dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="136dp"
        android:layout_height="40dp"
        android:layout_alignBaseline="@+id/editText18"
        android:layout_alignBottom="@+id/editText18"
        android:layout_alignStart="@+id/editText7"
        android:ems="10"
        android:hint="time"
        android:inputType="time" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="288dp"
        android:layout_height="45dp"
        android:layout_above="@+id/editText15"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="31dp"
        android:ems="10"
        android:inputType="none|textPersonName"
        android:text="Add Unavailability"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline" />

</RelativeLayout>